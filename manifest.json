{
    "manifest_version": 2,
    "name": "OpenCheck",
    "version": "1.0.0",
    "permissions": [
        "*://api.opencheck.is/*",
        "webRequest"
    ],
    "content_scripts": [
        {
            "matches": ["https://twitter.com/*"],
            "js": ["main.js"],
            "run_at": "document_end"
        }
    ]
}
